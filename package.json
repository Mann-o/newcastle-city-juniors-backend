{
  "name": "newcastle-city-juniors-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "refresh:deps": "rm -rf node_modules && rm package-lock.json && npm i",
    "refresh:db": "node ace migration:rollback && node ace migration:run && node ace db:seed"
  },
  "dependencies": {
    "@adonisjs/auth": "8.0.10",
    "@adonisjs/core": "5.4.1",
    "@adonisjs/lucid": "16.3.2",
    "@adonisjs/mail": "7.2.4",
    "@adonisjs/redis": "7.0.9",
    "@adonisjs/repl": "3.1.7",
    "@adonisjs/view": "6.1.1",
    "@sentry/node": "6.15.0",
    "@sentry/tracing": "6.15.0",
    "date-fns": "2.27.0",
    "faker": "5.5.3",
    "luxon": "2.1.1",
    "pg": "8.7.1",
    "phc-argon2": "1.1.2",
    "proxy-addr": "2.0.7",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "stripe": "8.191.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "5.3.8",
    "adonis-preset-ts": "2.1.0",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-adonis": "2.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "pino-pretty": "7.2.0",
    "prettier": "2.5.1",
    "typescript": "4.5.2",
    "youch": "2.2.2",
    "youch-terminal": "1.1.1"
  }
}
