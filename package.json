{
  "name": "newcastle-city-juniors-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "refresh:deps": "rm -rf node_modules && rm package-lock.json && npm i",
    "refresh:db": "node ace migration:rollback && node ace migration:run && node ace db:seed"
  },
  "dependencies": {
    "@adonisjs/auth": "8.1.0",
    "@adonisjs/core": "5.7.5",
    "@adonisjs/lucid": "17.2.0",
    "@adonisjs/mail": "8.0.1",
    "@adonisjs/redis": "7.2.0",
    "@adonisjs/repl": "3.1.10",
    "@adonisjs/view": "6.1.4",
    "@faker-js/faker": "6.2.0",
    "@sentry/node": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "date-fns": "2.28.0",
    "jsqr": "^1.4.0",
    "luxon": "2.3.2",
    "pg": "8.7.3",
    "phc-argon2": "1.1.3",
    "proxy-addr": "2.0.7",
    "qrcode": "^1.5.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "stripe": "8.219.0",
    "upng-js": "^2.1.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "5.6.2",
    "adonis-preset-ts": "2.1.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-adonis": "2.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "pino-pretty": "7.6.1",
    "prettier": "2.6.2",
    "typescript": "4.6.4",
    "youch": "2.2.2",
    "youch-terminal": "1.1.1"
  }
}
